{
  "name": "fin-tracker",
  "version": "1.0.0",
  "description": "A financial tracker.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf .cache dist",
    "build:background": "node ./src/background/background.config.js",
    "build:renderer": "node ./src/renderer/renderer.config.js",
    "build": "tsc && concurrently \"npm run build:renderer\" \"npm run build:background\"",
    "dev": "electron ./dist/main.js",
    "prettier": "prettier --write src/**/*.{ts,tsx,scss,html,js}",
    "precommit": "npm run prettier"
  },
  "author": "Dustin Jensen",
  "license": "ISC",
  "dependencies": {
    "electron": "^8.1.1",
    "evergreen-ui": "^4.23.3",
    "immer": "^6.0.1",
    "moment": "^2.22.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-portal": "^4.1.5",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "recharts": "^1.8.5",
    "redux": "^4.0.0",
    "redux-electron-ipc": "^1.1.12"
  },
  "devDependencies": {
    "@types/node": "^12.12.30",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "concurrently": "^3.5.1",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "1.19.1",
    "rimraf": "^2.6.2",
    "typescript": "^3.8.3"
  }
}
